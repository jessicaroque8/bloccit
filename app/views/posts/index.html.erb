<div class="page-header">
   <%=link_to "New Post", new_post_path, class: 'pull-right btn btn-success' %>
   <h1>All Posts</h1>
</div>

 <% @posts.each do |post| %>
   <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= link_to post.title, post %>
       </h4>
     </div>
   </div>
 <% end %>
